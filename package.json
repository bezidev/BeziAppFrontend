{
  "name": "beziapp",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build --outDir dist",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "dev": "vite --port 8080",
    "start": "sirv dist --port 3000 -H 0.0.0.0 --no-clear -s -G",
    "prepare-css": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile src/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile src/smui-dark.css -i src/theme/dark"
  },
